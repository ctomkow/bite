virsh list --all
virsh start ansible-test
virsh shutdown ansible-test
virsh destroy ansible-test # forcibly shutdown
(connect) virsh console <domain>
(connect) virsh console 4
(disconnect) CTRL + Shift + ]

# edit existing vm config
virsh edit ansible-test

# delete a vm
virsh undefine ansible-test
sudo rm /var/lib/libvirt/images/ansible-test.qcow2

# networking
virbr0 # nat (private ip)
br0 # bridged (vm get's own ip)
